<section class="socials" id="socialsðŸ”—">
	<h2>My Social accounts</h2>
	<div>
		<% if (socials.youtube) { %>
			<a class="socials__link" href="<%= socials.youtube %> " target="_blank"
				rel="noopener noreferrer" title="Open Youtube profile">
				<i class="fab fa-youtube"></i>
			</a>
		<% } %>
		<% if (socials.twitch) { %>
			<a class="socials__link" href="<%= socials.twitch %> " target="_blank"
				rel="noopener noreferrer" title="Open Twitch profile">
				<i class="fab fa-twitch"></i>
			</a>
		<% } %>
		<% if (socials.medium) { %>
			<a class="socials__link" href="<%= socials.medium %> " target="_blank"
				rel="noopener noreferrer" title="Open Medium profile">
				<i class="fab fa-medium"></i>
			</a>
		<% } %>
		<% if (socials.titter) { %>
			<a class="socials__link" href="<%= socials.twitter %> " target="_blank"
				rel="noopener noreferrer" title="Open Twitter profile">
				<i class="fab fa-twitter"></i>
			</a>
		<% } %>
		<% if (socials.facebook) { %>
			<a class="socials__link" href="<%= socials.facebook %> " target="_blank"
				rel="noopener noreferrer" title="Open Facebook profile">
				<i class="fab fa-facebook"></i>
			</a>
		<% } %>
		<% if (socials.linkedIn) { %>
			<a class="socials__link" href="<%= socials.linkedIn %> " target="_blank"
				rel="noopener noreferrer" title="Open LinkedIn profile">
				<i class="fab fa-linkedin"></i>
			</a>
		<% } %>
		<!-- github -->
		<a class="socials__link" href="https://github.com/<%= githubUsername %> " target="_blank"
			rel="noopener noreferrer"
			title="Open Github profile">
			<i class="fab fa-github"></i>
		</a>
	</div>
</section>